{
  "name": "mvcr",
  "version": "1.0.6",
  "description": "Minimum Viable Client Router",
  "repository": "git@github.com:wulfmann/mvcr.git",
  "author": "Joe Snell <joepsnell@gmail.com>",
  "license": "MIT",
  "private": false,
  "source": "src/mvcr.ts",
  "main": "dist/mvcr.js",
  "module": "dist/mvcr.mjs",
  "unpkg": "dist/mvcr.umd.js",
  "scripts": {
    "test": "jest",
    "bump": "standard-version",
    "build": "microbundle",
    "release": "git push --follow-tags origin master && npm publish",
    "publish": "npm run bump && npm run release"
  },
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.1",
    "gzip-size-cli": "^3.0.0",
    "jest": "^24.8.0",
    "microbundle": "^0.11.0",
    "standard-version": "^7.0.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3",
    "uglify-js": "^3.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "keywords": [
    "router",
    "spa",
    "microrouter"
  ]
}
